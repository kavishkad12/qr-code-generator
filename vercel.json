{
  "version": 2,
  "builds": [
    {
      "src": "api/generateQR.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/**/*",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/generateQR",
      "dest": "/api/generateQR.js"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/$1"
    }
  ]
}
